<!DOCTYPE html>
<html>
<head>
    <!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@3.3.7/dist/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

<!-- Optional theme -->
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@3.3.7/dist/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">

<!-- Latest compiled and minified JavaScript -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@3.3.7/dist/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    <title>User Details</title>
    <link rel="stylesheet"  href="table.css">
</head>
<body>
   
    <div class="container">
        <h1 style="color: rgb(40, 38, 165);">User Details</h1>
    <table>
        <thead>
            <tr>
                <th class="me">ID</th>
                <th>user_name</th>
                <th>user_email</th>
                <th>user_password</th>
                <th>user_image</th>
                <th>total_orders</th>
                <th>createtd_time</th>
                <!-- Add more table headers for other columns as needed -->
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td class="c"><%= user.user_id %></td>
                    <td><%= user.user_name %></td>
                    <td><%= user.user_email %></td>
                    <td ><%= user.user_password %></td>
                    <!-- <td ><%=  user.user_image %></td> -->
                    <td><span ><img src="<%=  user.user_image %>" class="image" alt="image"></span></td>
                    <td><%= user.total_orders %></td>
                    <td ><%= user.created_at %></td>
                    <!-- Add more table cells for other columns as needed -->
                </tr>
            <% }); %>
        </tbody>
    </table>
</div>
</body>
</html>
